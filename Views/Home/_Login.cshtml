@model LoginUser
<div class="jumbotron">
<h2>Welcome to the Dojo Activity Center</h2>
</div>

<form action="/login" method="post">
    <div class="form-group col-sm-12">
    
    <div class="form-group col-sm-12">
        <label>Email</label>
        <span asp-validation-for="LoginEmail"></span>
        <input type="text" name="LoginEmail" class="form-control" />
    </div>
    <div class="form-group col-sm-12"></div>
        <label>Password</label>
        <span asp-validation-for="LoginPassword"></span>
        <input type="password" name="LoginPassword" class="form-control" />
    </div>
    <input type="submit" class="btn btn-primary" />
</form>


